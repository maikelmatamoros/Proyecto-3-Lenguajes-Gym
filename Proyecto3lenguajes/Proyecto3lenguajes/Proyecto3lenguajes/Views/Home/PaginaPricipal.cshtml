@model Proyecto3lenguajes.Models.Administrador
@{
    ViewData["Title"] = "PaginaPricipal";
    Layout = "layoutAdm";
}


<h1>PaginaPricipal</h1>
<div class="row">
    <div class="col-md-4"  style="background-color:rgba(10, 129, 16, 0.5); background-repeat: no-repeat; background-attachment: fixed; ">
        <div class="card-body ">

            <img src="~/img/tiktok.png" class="img-fluid rounded mx-auto d-block" width="60" height="20" alt="Responsive image">
        </div>
        <form class="row justify-content-center">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>


            <div class="form-group row ">
                <label   style="color:rgba(255, 255, 255,4);">Descripcion:</label>
                <label id="descripcion" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label  style="color:rgba(255, 255, 255,4);">Ubicacion:</label>
                <label id="ubicacion" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row" >
                <label style="color:rgba(255, 255, 255,4);">Telefono:</label>
                <label id="telefono" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label  style="color:rgba(255, 255, 255,4);">Correo:</label>
                <label id="correo" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label  style="color:rgba(255, 255, 255,4);">Capacidad:</label>
                <label id="capacidad" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label  style="color:rgba(255, 255, 255,4);">Porcentaje:</label>
                <label id="porcentaje" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label style="color:rgba(255, 255, 255,4);">Hora apertura:</label>
                <label id="horaI" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="background-color:rgb(165, 11, 11); opacity:1">modificar</button>
            </div>
            <div class="form-group row">
                <label style="color:rgba(255, 255, 255,4);">Hora cierre:</label>
                <label id="horaC" style="color:rgba(255, 255, 255,4);"></label>
                <button type="button" class="btn btn-danger" style="        background-color: rgb(165, 11, 11); opacity: 1">modificar</button>
            </div>

        </form>
    </div>
</div>

